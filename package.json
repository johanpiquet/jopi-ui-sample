{
  "name": "jopi-ui-sample",
  "version": "0.0.1",
  "license": "MIT",
  "homepage": "https://github.com/johanpiquet/jopi-ui-sample",
  "keywords": ["express.js", "nextjs", "react", "react-ssr", "react-hydrate"],
  "author": "Johan PIQUET",
  "description": "Sample UI for Jopi Rewrite",
  "engines": {
    "bun": "^1.2.18",
    "node": ">=22.18.0"
  },
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist; npx tsc",
    "clean": "rm -rf ./dist; rm -rf .turbo; rm tsconfig.tsbuildinfo",
    "tsc": "npx tsc",
    "devNodeUI": "JOPI_LOG=0 JOPI_DEV=0 npx jopin dist/index.js",
    "devBunUI": "JOPI_LOG=0 JOPI_DEV=1 npx jopib src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "^1.2.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "jopi-rewrite": "workspace:^"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.544.0"
  },
  "jopi": {
    "webSiteUrl": "http://127.0.0.1:3000",
    "inlineMaxSize_ko": 10
  }
}
